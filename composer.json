{
    "name":        "dqe/turbolinks-bundle",
    "type":        "symfony-bundle",
    "description": "Turbolinks bundle for Symfony2",
    "keywords":    ["turbolinks", "jquery.turbolinks"],
    "homepage":    "https://helthe.co",
    "license":     "MIT",
    "authors": [
        {
            "name": "Carl Alexander",
            "email": "carlalexander@helthe.co"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3|~3.0|~4.0",
        "symfony/console": "~2.8|~3.4|~4.0",
        "symfony/process": "~3.4|~4.0",
        "composer/composer": "^1.6.3",
        "dqe/turbolinks": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dqesupport/Turbolinks.git"
        }
    ],
    "autoload": {
        "psr-4": { "Helthe\\Bundle\\TurbolinksBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
